<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shizhefei.eventbus.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.xmq.eventbus.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.xmq.eventbus.NextActivity" />
        <activity android:name="com.xmq.eventbus.FragmentTestActivity" />
        <activity android:name="com.xmq.eventbus.ThreadTestActivity" />

        <meta-data
            android:name="channel"
            android:value="luckincoffee" />
    </application>

</manifest>